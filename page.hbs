{{!< default}}

{{#post}}
<h1 class="w3-center">{{title}}</h1>
{{#match @page.show_title_and_feature_image}}
    {{#if feature_image}}
        <figure class="postImg w3-mobile w3-center">
        <img src="{{img_url feature_image}}" alt="{{title}}"
            class="w3-mobile" loading="lazy">
    </figure>
    {{/if}}
{{/match}}
{{content}}
{{/post}}